{
  "name": "@tsed/basic-example",
  "version": "1.0.0",
  "description": "Here an example to configure your server and how to create your first controller.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "./node_modules/.bin/tsc -b",
    "test": "npx tsc -b && jest dist/.*test.js",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "npx tsc -b && node dist/index.js",
    "debug": "./node_modules/.bin/tsnd --inspect --respawn ./src/index.ts",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "6.11.3",
    "@tsed/common": "6.11.3",
    "@tsed/core": "6.11.3",
    "@tsed/di": "6.11.3",
    "@tsed/exceptions": "6.11.3",
    "@tsed/json-mapper": "6.11.3",
    "@tsed/schema": "6.11.3",
    "@tsed/swagger": "6.11.3",
    "@tsed/platform-express": "6.11.3",
    "@tsed/typeorm": "6.11.3",
    "@types/swagger-schema-official": "2.0.21",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "@types/http-proxy": "1.17.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.45",
    "@types/request-promise": "4.1.45",
    "@types/sinon": "9.0.8",
    "@types/sinon-chai": "3.2.4",
    "@types/supertest": "2.0.10",
    "axios": "^0.21.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "5.3.0",
    "jest": "^26.6.3",
    "mocha": "8.1.3",
    "nodemon": "^1.19.4",
    "nyc": "15.1.0",
    "prettier": "^2.3.0",
    "rimraf": "3.0.0",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "supertest": "6.0.0",
    "ts-node": "^9.0.0",
    "tslint": "6.1.3",
    "typescript": "^3.9",
    "yarn": "^1.22.10"
  }
}
