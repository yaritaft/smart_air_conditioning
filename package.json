{
  "name": "smart-air-conditioning",
  "version": "1.0.0",
  "description": "Smart air conditioning",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "tsc --project tsconfig.compile.json",
    "test": "npm run test:lint && npm run test:coverage",
    "test:compiled": "npm run build && jest --runInBand dist/.*test.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "start:prod": "npm run build && node dist/index.js",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only ./src/index.ts",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "docker:build": "yarn build && docker-compose build",
    "typeorm": "./node_modules/.bin/tsc -b && ts-node ./node_modules/typeorm/cli.js",
    "migrations:create": "npm run typeorm migration:generate -- -n ${npm_config_migration_name}",
    "migrations:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migrations:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "seed:generate": "npm run typeorm migration:generate -- -n ${npm_config_migration_name} -c seed",
    "seed:run": "npm run typeorm migration:run -- -c seed",
    "seed:revert": "npm run typeorm migration:revert -- -c seed"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "6.95.1",
    "@tsed/common": "6.95.1",
    "@tsed/core": "6.95.1",
    "@tsed/di": "6.95.1",
    "@tsed/exceptions": "6.95.1",
    "@tsed/json-mapper": "6.95.1",
    "@tsed/platform-express": "6.95.1",
    "@tsed/schema": "6.95.1",
    "@tsed/swagger": "6.95.1",
    "@tsed/typeorm": "6.95.1",
    "@types/swagger-schema-official": "2.0.21",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "^4.17.1",
    "http-status-code": "^2.1.0",
    "http-status-codes": "^2.1.4",
    "method-override": "^3.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "@types/http-proxy": "1.17.2",
    "@types/jest": "^26.0.23",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.45",
    "@types/request-promise": "4.1.45",
    "@types/supertest": "2.0.10",
    "@types/uuid": "^8.3.3",
    "axios": "^0.21.1",
    "concurrently": "5.3.0",
    "jest": "^26.6.3",
    "nodemon": "^1.19.4",
    "prettier": "^2.3.0",
    "rimraf": "3.0.0",
    "supertest": "6.0.0",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.1.8",
    "tslint": "6.1.3",
    "typescript": "^3.9"
  }
}
